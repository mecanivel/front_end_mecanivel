{
   "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "android": {
    "usesCleartextTraffic": true,
    "permissions": ["INTERNET"]
},
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/(tabs)/index.jsx", "pages/HomeScreen.jsx"  ]
}
